local function YZKVIM_fake_script() -- INFAMMO.ESP 
	local script = Instance.new('LocalScript', INFAMMO)

	local enabled = false
	
	script.Parent.Activated:Connect(function()
		if not enabled then
		    enabled = true
			script.Parent.BackgroundColor3 = Color3.fromRGB(255, 154, 12)
			for _, v in pairs(game.ReplicatedStorage.Weapons:GetChildren()) do
				if v:FindFirstChild("Ammo") then
					if not v:FindFirstChild("OldValueAmmo") then
						local oldvalue = Instance.new("NumberValue")
						oldvalue.Value = v.Ammo.Value
						oldvalue.Parent = v
						oldvalue.Name = "OldValueAmmo"
					end
					v.Ammo.Value = 10000000
				end
				if v:FindFirstChild("StoredAmmo") then
					if not v:FindFirstChild("OldValueStoredAmmo") then
						local oldvalue = Instance.new("NumberValue")
						oldvalue.Value = v.StoredAmmo.Value
						oldvalue.Parent = v
						oldvalue.Name = "OldValueStoredAmmo"
					end
					v.StoredAmmo.Value = 10000000
				end
			end
		else
			enabled = false
			script.Parent.BackgroundColor3 = Color3.fromRGB(255,255,255)
			for _, v in pairs(game.ReplicatedStorage.Weapons:GetChildren()) do
				if v:FindFirstChild("Ammo") then
					if v:FindFirstChild("OldValueAmmo") then
						v.Ammo.Value = v.OldValueAmmo.Value
					end
				end
				if v:FindFirstChild("StoredAmmo") then
					if v:FindFirstChild("OldValueStoredAmmo") then
						v.StoredAmmo.Value = v.OldValueStoredAmmo.Value
					end
				end
			end
		end
	end)
end
coroutine.wrap(YZKVIM_fake_script)()
local function MZTJJHJ_fake_script() -- Chams.ESP 
	local script = Instance.new('LocalScript', Chams)

	local enabled = false
	
	script.Parent.Activated:Connect(function()
		if not enabled then
			enabled = true
			script.Parent.BackgroundColor3 = Color3.fromRGB(255, 154, 12)
		else
			enabled = false
			script.Parent.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		end
	end)
	
	game:GetService("RunService").RenderStepped:Connect(function()
		if enabled then
			for _, v in pairs(game.Players:GetChildren()) do
				if v.Character then
					if v:IsA("Player") then
						if v.Name ~= game.Players.LocalPlayer.Name then
							if v.Team ~= game.Players.LocalPlayer.Team then
								local esp = Instance.new("BoxHandleAdornment")
								esp.Color3 = script.Parent.Parent.Parent.ColorPicker.Preview.BackgroundColor3
								esp.Parent = v.Character.Head
								esp.Adornee = v.Character.Head
								esp.AlwaysOnTop = true
								esp.ZIndex = 2
								game:GetService("Debris"):AddItem(esp,0.1)
								for _, e in pairs(v.Character:GetChildren()) do
									if e:IsA("Part") or e:IsA("MeshPart") then
										local esp = Instance.new("BoxHandleAdornment")
										esp.Parent = e
										esp.ZIndex = 2
										esp.AlwaysOnTop = true
										esp.Adornee = e
										esp.Size = e.Size
										esp.Color3 = script.Parent.Parent.Parent.ColorPicker.Preview.BackgroundColor3
										game:GetService("Debris"):AddItem(esp,0.1)
									end
								end
							end
						end
					end
				end
			end
		end
	end)
end
coroutine.wrap(MZTJJHJ_fake_script)()
local function THSNIFL_fake_script() -- ImageButton.LocalScript 
	local script = Instance.new('LocalScript', ImageButton)

	local activated = false
	
	script.Parent.Activated:Connect(function()
		if not activated then
			activated = true
			script.Parent.Parent.Parent.Parent.ColorPicker.Visible = true
		else
			activated = false
			script.Parent.Parent.Parent.Parent.ColorPicker.Visible = false
		end
	end)
end
coroutine.wrap(THSNIFL_fake_script)()
local function ELKC_fake_script() -- DistanceESP.ESP 
	local script = Instance.new('LocalScript', DistanceESP)

	local enabled = false
	
	script.Parent.Activated:Connect(function()
		if not enabled then
			enabled = true
			script.Parent.BackgroundColor3 = Color3.fromRGB(255, 154, 12)
		else
			enabled = false
			script.Parent.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		end
	end)
	
	game:GetService("RunService").RenderStepped:Connect(function()
		if enabled then
			for _, v in pairs(game.Players:GetChildren()) do
				if v.Character then
					if v:IsA("Player") then
						if v.Name ~= game.Players.LocalPlayer.Name then
							if v.Team ~= game.Players.LocalPlayer.Team then
								if game.Workspace:FindFirstChild(game.Players.LocalPlayer.Name) then
									if v.Character:FindFirstChild("HumanoidRootPart") then
										local magtude = (game.Workspace.CurrentCamera.CFrame.p - v.Character.HumanoidRootPart.CFrame.p).Magnitude
										local BillboardGui = Instance.new("BillboardGui")
										local TextLabel = Instance.new("TextLabel")
	
										BillboardGui.Parent = v.Character.HumanoidRootPart
										BillboardGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
										BillboardGui.Active = true
										BillboardGui.AlwaysOnTop = true
										BillboardGui.Size = UDim2.new(4, 0, 2, 0)
										BillboardGui.StudsOffsetWorldSpace = Vector3.new(0, -4, 0)
	
										TextLabel.Parent = BillboardGui
										TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
										TextLabel.BackgroundTransparency = 1.000
										TextLabel.Size = UDim2.new(1, 0, 1, 0)
										TextLabel.Font = Enum.Font.Ubuntu
										TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
										TextLabel.TextScaled = true
										TextLabel.TextSize = 14.000
										TextLabel.TextWrapped = true
										TextLabel.Text = math.floor(magtude)
										game:GetService("Debris"):AddItem(BillboardGui, 0.01)
									end
								end
							end
						end
					end
				end
			end
		end
	end)
end
coroutine.wrap(ELKC_fake_script)()
