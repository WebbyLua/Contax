local function OCYTQUZ_fake_script() -- Mouse.LocalScript 
	local script = Instance.new('LocalScript', Mouse)

	local mouse = game.Players.LocalPlayer:GetMouse()
	
	game:GetService("RunService").RenderStepped:Connect(function()
		script.Parent.Position = UDim2.new(0,mouse.X,0,mouse.Y)
		if script.Parent.Parent.MainFrame.Visible then
			script.Parent.Visible = true
		else
			script.Parent.Visible = false
		end
	end)
end
coroutine.wrap(OCYTQUZ_fake_script)()
