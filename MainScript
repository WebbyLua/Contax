coroutine.wrap(BFYVBV_fake_script)()
local function IOXP_fake_script() -- norecoil.ESP 
	local script = Instance.new('LocalScript', norecoil)

	local enabled = false
	
	script.Parent.Activated:Connect(function()
		if not enabled then
			enabled = true
			script.Parent.BackgroundColor3 = Color3.fromRGB(255, 154, 12)
			for _, v in pairs(game.ReplicatedStorage.Weapons:GetChildren()) do
				if v:FindFirstChild("Recoil") then
					local oldvalue = Instance.new("NumberValue")
					oldvalue.Value = v.Recoil.Value
					oldvalue.Parent = v
					oldvalue.Name = "OldValueRecoil"
					if not v:FindFirstChild("OldValueRecoilX") then
						if v.Recoil:FindFirstChild("AngleY") then
							local oldvalue = Instance.new("NumberValue")
							oldvalue.Value = v.Recoil.AngleX.Value
							oldvalue.Parent = v
							oldvalue.Name = "OldValueRecoilX"
						end
						if v.Recoil:FindFirstChild("AngleY") then
							local oldvalue = Instance.new("NumberValue")
							oldvalue.Value = v.Recoil.AngleY.Value
							oldvalue.Parent = v
							oldvalue.Name = "OldValueRecoilY"
						end
					end
					v.Recoil.AngleY.Value = 0
					v.Recoil.AngleX.Value = 0
					v.Recoil.Value = 0
				end
			end
		else
			enabled = false
			script.Parent.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			for _, v in pairs(game.ReplicatedStorage.Weapons:GetChildren()) do
				if v:FindFirstChild("Recoil") then
					if v:FindFirstChild("OldValueRecoil") then
						v.Recoil.AngleX.Value = v.OldValueRecoilX.Value
						v.Recoil.AngleY.Value = v.OldValueRecoilY.Value
						v.Recoil.Value = v.OldValueRecoil.Value
					end
				end
			end
		end
	end)
end
coroutine.wrap(IOXP_fake_script)()
local function TWEJ_fake_script() -- fastfire.ESP 
	local script = Instance.new('LocalScript', fastfire)

	local enabled = false
	
	script.Parent.Activated:Connect(function()
		if not enabled then
		    enabled = true
			script.Parent.BackgroundColor3 = Color3.fromRGB(255, 154, 12)
			for _, v in pairs(game.ReplicatedStorage.Weapons:GetChildren()) do
				if v:FindFirstChild("FireRate") then
					if not v:FindFirstChild("OldValueFireRate") then
						local oldvalue = Instance.new("NumberValue")
						oldvalue.Value = v.FireRate.Value
						oldvalue.Parent = v
						oldvalue.Name = "OldValueFireRate"
					end
					v.FireRate.Value = 0.005
				end
			end
		else
			enabled = false
			script.Parent.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			for _, v in pairs(game.ReplicatedStorage.Weapons:GetChildren()) do
				if v:FindFirstChild("FireRate") then
					if v:FindFirstChild("OldValueFireRate") then
						v.FireRate.Value = v.OldValueFireRate.Value
					end
				end
			end
		end
	end)
end
